<AnchorPane id="AnchorPane" prefHeight="630.0" prefWidth="1240.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="finalproject.LogInController">
   <children>
      <ImageView fitHeight="630.0" fitWidth="1250.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../TabImgages/Imgaes%20(5).jpg" />
         </image>
      </ImageView>
      <AnchorPane layoutX="699.0" prefHeight="630.0" prefWidth="546.0">
         <children>
            <Label layoutX="233.0" layoutY="234.0" opacity="0.85" text="Sign In" textFill="WHITE">
               <font>
                  <Font name="Arial Italic" size="26.0" />
               </font>
            </Label>
            <TextField fx:id="EATextField" layoutX="115.0" layoutY="273.0" onMouseClicked="#EAOnAction" opacity="0.85" prefHeight="25.0" prefWidth="319.0" promptText="user/email address...">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></TextField>
            <TextField fx:id="PTextField" layoutX="115.0" layoutY="307.0" onMouseClicked="#POnAction" opacity="0.85" prefHeight="25.0" prefWidth="319.0" promptText="password...">
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></TextField>
            <Button alignment="CENTER" layoutX="211.0" layoutY="340.0" mnemonicParsing="false" onAction="#LogIn" opacity="0.85" prefHeight="25.0" prefWidth="126.0" text="Log In">

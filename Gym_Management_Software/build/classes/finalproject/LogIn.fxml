                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Hyperlink layoutX="224.0" layoutY="365.0" onAction="#ForgotPassword" prefHeight="31.0" prefWidth="100.0" text="Forgot password." />
            <Hyperlink layoutX="247.0" layoutY="381.0" onAction="#SignIn" prefHeight="39.0" prefWidth="54.0" text="Sign Up." />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
